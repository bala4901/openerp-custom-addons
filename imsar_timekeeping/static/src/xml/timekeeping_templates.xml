<?xml version="1.0" encoding="UTF-8"?>

<templates>

    <div t-name="imsar_timekeeping.stopwatch" class="oe_timekeeping_example timekeeping_stopped">
        <h4 class="timekeeping_timer">00:00:00</h4>
        <p class="timekeeping_start">
            <button type="button">Start</button>
        </p>
        <p class="timekeeping_stop">
            <button type="button">Stop</button>
        </p>
    </div>

    <div t-name="imsar_timekeeping.WeeklySummary" class="oe_timekeeping">
        <div class="testline" id="testline_id"><p>Bad</p></div>
        <table>
            <tr t-foreach="widget.accounts" t-as="account">
                <td>Account <t t-esc="account"/></td>
            </tr>
        </table>

    </div>

</templates>
<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Unnecessary Views -->
        <!--<record id="account.account_type_income_view1" model="account.account.type">-->
            <!--<field name="active" eval="False"/>-->
        <!--</record>-->
        <record id="account.account_type_expense_view1" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="account.account_type_asset_view1" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="account.account_type_liability_view1" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="account.account_type_income_view1" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <!-- Unused base account stuff (replaced by l10n_us) -->
        <record id="account.data_account_type_receivable" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="account.data_account_type_payable" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="account.data_account_type_income" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="account.data_account_type_expense" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <!-- Asset -->
        <record id="l10n_us.user_type_account_receivable" model="account.account.type">
            <field name="name">Account Receivable</field>
            <field name="code">account-rec</field>
            <field name="report_type">asset</field>
            <field name="close_method">unreconciled</field>
        </record>        
        <record id="l10n_us.user_type_current_asset" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_cash" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_other_current_asset" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_fixed_asset" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_accumulated_depreciation" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_other_asset" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <!-- liability -->
        <record id="l10n_us.user_type_account_payable" model="account.account.type">
            <field name="name">Account Payable</field>
            <field name="code">account-payable</field>
            <field name="report_type">liability</field>
            <field name="close_method">unreconciled</field>
        </record>
        <record id="l10n_us.user_type_credit_card" model="account.account.type">
            <field name="name">Credit Card</field>
            <field name="code">credit-card</field>
            <field name="report_type">liability</field>
            <field name="close_method">balance</field>
        </record>
        <record id="l10n_us.user_type_liability" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_current_liability" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_other_current_liability" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_long_term_liability" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <!-- Equity -->
        <record id="l10n_us.user_type_equity" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <!-- Income -->
        <record id="l10n_us.user_type_income" model="account.account.type">
            <field name="active" eval="False"/>
        </record>
        <record id="l10n_us.user_type_other_income" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <!-- Expense -->
        <record id="l10n_us.user_type_expense" model="account.account.type">
            <field name="active" eval="False"/>
        </record>

        <record id="l10n_us.user_type_other_expense" model="account.account.type">
            <field name="name">Other Expense</field>
            <field name="code">other-expense</field>
            <field name="report_type">expense</field>
            <field name="close_method">none</field>
        </record>

        <record id="l10n_us.user_type_cogs" model="account.account.type">
            <field name="name">Cost of Goods Sold</field>
            <field name="code">cogs</field>
            <field name="report_type">expense</field>
            <field name="close_method">none</field>
        </record>

        <!--record model="account.tax.code.template" id="tax_code">
            <field name="name">Tax</field>
        </record-->        
